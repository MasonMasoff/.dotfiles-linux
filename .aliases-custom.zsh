# Custom aliases
# Add your personal aliases, overriding those provided by the system or other plugins

# Example aliases
alias reload="source ~/.zshrc"
alias dotfiles="cd ~/.dotfiles-linux"

# Git aliases
alias gs="git status"
alias ga="git add"
alias gc="git commit -m"
alias gp="git push"
alias gl="git pull"

# Directory navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."

